{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "import/extensions": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": "off",
        "max-len": [
            "error",
            150
        ],
        "prefer-destructuring": "off"
    }
}